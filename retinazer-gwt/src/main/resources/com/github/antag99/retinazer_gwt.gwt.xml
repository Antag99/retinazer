<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='com.github.antag99.retinazer.gwt'>
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.github.antag99.retinazer"/>
    <inherits name="com.badlogic.gdx"/>
    <inherits name="com.badlogic.gwtref.GwtReflect"/>
    <extend-configuration-property name="gdx.reflect.include" value="com.github.antag99.retinazer"/>
    <extend-configuration-property name="gdx.reflect.include" value="com.github.antag99.retinazer.utils"/>
    <super-source path="retinazer/gwt/emu"/>
    <source path="gwt">
        <exclude name="**/emu/**"/>
    </source>
</module>
